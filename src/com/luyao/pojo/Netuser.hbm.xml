<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.luyao.pojo.Netuser" table="netuser" schema="sshbaidu">
        <id name="username">
            <column name="username" sql-type="varchar(250)" length="250"/>
        </id>
        <property name="password">
            <column name="password" sql-type="varchar(33)" length="33" />
        </property>
        <property name="usertype">
            <column name="usertype" sql-type="int(11)" />
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(50)" length="50" />
        </property>
        <property name="activeCode">
            <column name="activeCode" sql-type="varchar(32)" length="32" />
        </property>
        <property name="capacity">
            <column name="capacity" sql-type="bigint(255)" />
        </property>
    </class>
</hibernate-mapping>